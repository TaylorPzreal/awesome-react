{
  "name": "awesome-react",
  "version": "1.0.0",
  "description": "A basic web app from React, called awesome-react",
  "scripts": {
    "start": "webpack-dev-server --config config/webpack.dev.js --hot",
    "start-browser": "yarn run start --open",
    "build": "webpack --config config/webpack.prod.js --progress --profile",
    "build-analyse": "yarn run build --json > docs/stats.json",
    "dll": "rm -rf dll && webpack --config config/webpack.dll.js",
    "test": "jest --env=jsdom",
    "lint": "eslint --ext .jsx --ext .js src/",
    "format": "npm-run-all format-*",
    "format-jsx": "prettier src/**/*.js src/**/*.jsx --trailing-comma=es5 --write && yarn run lint --fix",
    "format-style": "prettier src/**/*.css src/**/*.scss --write",
    "server": "http-server ./dist/ -p 9000",
    "doc": "esdoc",
    "server-doc": "http-server ./doc -p 3000",
    "release": "standard-version",
    "coverage": "jest --coverage && cat ./coverage/lcov.info | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TaylorPzreal/awesome-react.git"
  },
  "keywords": [
    "react",
    "awesome-react",
    "react-app",
    "react-product"
  ],
  "author": "TaylorPzreal <taylorpzreal@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/TaylorPzreal/awesome-react/issues"
  },
  "homepage": "https://github.com/TaylorPzreal/awesome-react#readme",
  "dependencies": {
    "prop-types": "^15.7.2",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-loadable": "^5.5.0",
    "react-redux": "^7.1.0",
    "react-router-dom": "^5.0.1",
    "redux": "^4.0.4",
    "redux-devtools": "^3.5.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.5.5",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "@commitlint/cli": "^8.1.0",
    "@commitlint/config-conventional": "^8.1.0",
    "@commitlint/prompt": "^8.1.0",
    "babel-eslint": "^10.0.2",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.0.4",
    "coveralls": "^3.0.6",
    "css-loader": "^3.2.0",
    "cssnano": "^4.1.10",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "esdoc": "^1.1.0",
    "esdoc-jsx-plugin": "^1.0.0",
    "esdoc-react-plugin": "^1.0.1",
    "esdoc-standard-plugin": "^1.0.0",
    "eslint": "^6.2.1",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-loader": "^2.2.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^22.15.2",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.14.3",
    "file-loader": "^4.2.0",
    "html-webpack-plugin": "^4.0.0-beta.8",
    "http-server": "^0.11.1",
    "husky": "^3.0.4",
    "interpolate-webpack-plugin": "^2.1.0",
    "jest": "^24.9.0",
    "lint-staged": "^9.2.3",
    "mini-css-extract-plugin": "^0.8.0",
    "npm-run-all": "^4.1.5",
    "postcss-import": "^12.0.1",
    "postcss-js": "^2.0.2",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^1.18.2",
    "sass": "^1.22.10",
    "sass-loader": "^7.3.1",
    "standard-version": "^7.0.0",
    "style-loader": "^1.0.0",
    "terser-webpack-plugin": "^1.4.1",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "url-loader": "^2.1.0",
    "webpack": "^4.39.2",
    "webpack-bundle-analyzer": "^3.4.1",
    "webpack-cli": "^3.3.7",
    "webpack-dashboard": "^3.0.7",
    "webpack-dev-server": "^3.8.0",
    "webpack-manifest-plugin": "^2.0.4",
    "webpack-merge": "^4.2.1"
  },
  "lint-staged": {
    "src/**/*.{js,jsx}": [
      "format-jsx",
      "git add"
    ],
    "src/**/*.{css,scss}": [
      "format-style",
      "git add"
    ]
  },
  "standard-version": {
    "changelogHeader": "# Changelog\n\n awesome-react\n"
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/prompt"
    }
  }
}
