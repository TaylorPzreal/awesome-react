{
  "root": true,
  "extends": ["airbnb"],
  "parserOptions": {
    "ecmaVersion": 6
  },
  "env": {
    "node": true,
    "commonjs": true
  },
  "rules": {
    "comma-dangle": 0,
    "import/no-extraneous-dependencies": 0,
    "object-shorthand": 0,
    "space-before-function-paren": 0,
    "no-underscore-dangle": 0,
    "import/no-dynamic-require": 0
  },
  "plugins": [
  ],
  "globals": {}
}
